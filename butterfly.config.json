{
  "appName": "wanderBloom",
  "version": "1.0.0",
  "deployment": {
    "environment": "production",
    "region": "us-west-2",
    "instanceType": "t2.micro"
  },
  "analytics": {
    "trackingId": "YOUR_BUTTERFLY_TRACKING_ID",
    "events": [
      "app_initialized",
      "form_submitted",
      "recommendations_received",
      "error_occurred"
    ]
  },
  "monetization": {
    "tier": "free",
    "features": [
      "basic_analytics",
      "user_tracking",
      "recommendation_engine"
    ]
  },
  "workflow": {
    "autoScale": true,
    "maxInstances": 3,
    "minInstances": 1
  }
} 